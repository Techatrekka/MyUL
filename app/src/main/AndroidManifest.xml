        android:usesCleartextTraffic="true" >
        <activity android:name=".ContactInfo" />
        <activity android:name=".InfoActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".UserType" />
        <activity android:name=".HomeActivity" />
